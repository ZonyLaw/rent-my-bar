<div class="container">
  <h1>Welcome to <%= @bar.address %>'s <%= @bar.name %></h1>

  <% @bar.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>
  <p><%=@bar.address%></p>
  <p><%= @bar.description %></p>
  <h3 class="notice"><%= notice %></h3>

  <%=link_to "Home", root_path %> |
  <%=link_to "Back to List", bars_path(@bar) %> |
  <%=link_to "Book Venue", new_bar_booking_path(@bar) %>

</div>
